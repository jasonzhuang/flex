<?xml version="1.0" encoding="utf-8"?>
<s:Application
	xmlns:fx="http://ns.adobe.com/mxml/2009"
	xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:mx="library://ns.adobe.com/flex/mx"
 	xmlns:components="components.*"
	initialize="init()"
	>
	<fx:Script>
		<![CDATA[
			private function init():void {
			}
			
			private function imgComplete(event:Event):void {
				trace(event.target);
				trace("complete handler");
			}
			
			private function imgCreated(event:Event):void {
				trace(event.target);
				trace("creationComplete handler");
			}
		]]>
	</fx:Script>
	<s:Image source="@Embed('assets/1.jpg')" complete="imgComplete(event)" creationComplete="imgCreated(event)"/>
</s:Application>
